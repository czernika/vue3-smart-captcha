        "@vitejs/plugin-vue": "^5.2.1",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "^8.57.1",
        "eslint-plugin-vue": "^9.32.0",
        "husky": "^9.1.7",
        "jsdom": "^25.0.1",
        "lint-staged": "^15.2.10",
        "typescript": "5.6.2",
        "vite": "^5.4.11",
        "vite-plugin-dts": "^3.9.1",
        "vitest": "^1.6.0",
