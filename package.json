{
    "name": "vue3-smart-captcha",
    "description": "Yandex Smart Captcha for Vue3 projects",
    "keywords": [
        "captcha",
        "component",
        "smart-captcha",
        "vue",
        "vue3"
    ],
    "license": "MIT",
    "version": "1.1.4",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/SmartCaptcha.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
